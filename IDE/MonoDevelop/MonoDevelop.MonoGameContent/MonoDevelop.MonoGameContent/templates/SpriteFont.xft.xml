<?xml version="1.0"?>
<Template>
    <TemplateConfiguration>
            <_Name>Sprite Font</_Name>
            <Icon>monogame-icon</Icon>
            <_Category>MonoGame</_Category>
            <_Description>MonoGame SpriteFont Template</_Description>
    </TemplateConfiguration>
    <TemplateFiles>
        <File DefaultExtension=".spritefont" DefaultName="Font">
<![CDATA[<?xml version="1.0" encoding="utf-8"?>
