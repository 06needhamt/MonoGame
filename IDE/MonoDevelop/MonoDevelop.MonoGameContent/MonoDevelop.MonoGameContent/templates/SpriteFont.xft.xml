  <Asset Type="Graphics:FontDescription">
        <FontName>Segoe UI Mono</FontName>
        <Size>14</Size>
        <Spacing>0</Spacing>
        <UseKerning>true</UseKerning>
        <Style>Regular</Style>
        <!-- <DefaultCharacter>*</DefaultCharacter> -->
        <CharacterRegions>
        <CharacterRegion>
        <Start>&#32;</Start>
        <End>&#126;</End>
      </CharacterRegion>
    </CharacterRegions>
  </Asset>
</XnaContent>]]>
        </File>
    </TemplateFiles>
    <FileOptions/>
</Template>